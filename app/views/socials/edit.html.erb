<div class="hero-unit" style="background:white;min-height:700px;">
  <h2 style="color:#890000"><%= @social.heading %></h2>
  <font style="font-size:14px;color:rgb(150, 150, 150);font-family:georgia">
  Place : <%= @social.country %> | Updated on : <%= @social.created_at.strftime("%d %b. %Y") %>
  </font>
  <p>
  <%= image_tag @social.photo, :style =>'float:right;margin-left:10px;
  max-height:600px;min-height:400px;' %>
  <%= @social.text %>
  </p>
</div>
<style type="text/css">
p {font-family: georgia;
line-height: 1.5em;
text-align: justify;
font-size: 16px;}
