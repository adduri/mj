            <% unless Social.where(:category => "social", :verify => true).where('start <= ?', Date.tomorrow).where('till >= ?', Date.today).last.blank? %>
              <%= image_tag  Social.where(:category => "social", :verify => true).where('start <= ?', Date.tomorrow).where('till >= ?', Date.today).last.photo, :style => 'width:200px;height:200px;border-radius:3px;' %>
              <%= Social.where(:category => "social", :verify => true).where('start <= ?', Date.tomorrow).where('till >= ?', Date.today).last.heading%>
            <% else %>
              <img src="http://www.campusactivitiesmagazine.com/images/yourAdHere_vertical_1.png" width="152" border-radius="3px">
