<% if current_user.user_id == 0 %>
<div>
  <a class="btn btn-danger" href="/users?begin=15">Users</a>
  <a class="btn btn-danger" href="/family_members">Family members</a>
  <a class="btn btn-danger" href="/relations">Relations</a>
  <a class="btn btn-danger" href="/feedbacks">View Feedbacks</a>
  <a class="btn btn-danger" href="/publish_event">Publish Event</a>
</div>
<% else %>
  <div >
  <a class="btn btn-danger" href="/users/edit_profile/<%= current_user.id %>">Profile</a>
  <a class="btn btn-danger" href="/family_members/family/<%= current_user.id %>">Family Tree</a>
  <a class="btn btn-danger" href="/feedbacks">Feedback</a>
  <a class="btn btn-danger" href="/feedbacks/new">Give feedback</a>
</div>  
 




<% end %>
<h2 style="color:#890000">Feedback</h2>

<table>
  <tr>
    <th>MJ ID</th>
    <th>Details</th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.user.mj_id %></td>
    <td><%= feedback.details %></td>
   
  </tr>
<% end %>
</table>
